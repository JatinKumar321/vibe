{
  "extends": "./node_modules/gts/",
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "no-unused-vars": "off",
    "n/no-unsupported-features/node-builtins": ["error", {
      "version": ">=14.0.0"
    }]
  },
  "overrides": [
    {
      "files": ["**/*.test.ts", "**/*.spec.ts", "**/tests/**"],
      "rules": {
        "n/no-unpublished-import": "off",
        "@typescript-eslint/no-unused-vars": "off",
        "no-unused-vars": "off"
      }
    }
  ]
}